-- 1. Inspect database structure
-- ================================

-- sqlite_master is SQLite's internal catalog.
-- This query asks: &quot;What tables exist in this database?&quot;
-- We use it to confirm our schema ran successfully.
SELECT name FROM sqlite_master WHERE type='table';

-- ================================
-- 2. Insert a workout session
-- ================================
INSERT INTO workout_sessions (
  workout_date,
  workout_type,
  duration_minutes,
  active_calories,
  notes
)
VALUES (
  '2025-12-26',
  'running',
  50,
  388,
  '5k distance'
);

-- ================================
-- 3. Retrieve all workouts
-- ================================
SELECT * FROM workout_sessions;

-- ================================
-- 4. Retrieve selected columns + ordering
-- ================================
SELECT workout_date, workout_type, duration_minutes, active_calories
FROM workout_sessions
ORDER BY workout_date DESC;

-- ================================
-- 5. Insert activity-specific details
-- ================================
INSERT INTO activity_details (
  workout_id,
  distance_miles,
  calories_segment
)
VALUES (
  2,
  3.24,
  470
);

-- ================================
-- 6. Retrieve activity details alone
-- ================================
SELECT * FROM activity_details;



-- ================================
-- 7. Combine workouts + details (JOIN)
-- ================================
SELECT
  ws.workout_date,
  ws.workout_type,
  ws.duration_minutes,
  ws.active_calories,
  ad.distance_miles,
  ad.calories_segment
FROM workout_sessions ws
LEFT JOIN activity_details ad
  ON ws.id = ad.workout_id
ORDER BY workout_date DESC;




